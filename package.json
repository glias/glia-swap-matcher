{
  "name": "glia-swap-matcher",
  "version": "0.1.0",
  "description": "",
  "bin": "lib/index.js",
  "engines": {
    "node": ">=12 <13"
  },
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development ts-node src/index",
    "start:prod": "cross-env NODE_ENV=production ts-node src/index",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "build:dev": "cross-env NODE_ENV=development yarn run tsc",
    "build:prod": "cross-env NODE_ENV=production yarn run tsc",
    "clean": "npx rimraf lib && npx rimraf tsconfig.tsbuildinfo && npx rimraf src/migrations",
    "format": "prettier -w --ignore-unknown src/**/*",
    "db:init:dev": "npm run build:dev && typeorm migration:generate -c development -d src/migrations -n InitDb && npm run build:dev && typeorm -c development migration:run",
    "db:init:prod": "npm run build:prod && typeorm migration:generate -c production -d src/migrations -n InitDb && npm run build:prod && typeorm -c production migration:run"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*": "npm run format"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:glias/glia-swap-matcher.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@ckb-lumos/base": "0.14.2-rc6",
    "@types/cron": "1.7.2",
    "@types/jest": "26.0.19",
    "@types/node": "14.14.16",
    "@types/socket.io": "2.1.11",
    "cross-env": "7.0.3",
    "fetch-mock": "9.10.7",
    "fetch-mock-jest": "1.3.0",
    "husky": "4.3.6",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "nodemon": "2.0.6",
    "prettier": "2.2.1",
    "ts-jest": "26.4.4",
    "ts-node": "9.0.0",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "@ckb-lumos/sql-indexer": "0.14.2-rc6",
    "@nervosnetwork/ckb-sdk-core": "0.39",
    "@nervosnetwork/ckb-sdk-rpc": "0.39",
    "@nervosnetwork/ckb-sdk-utils": "0.39",
    "bigint-isqrt": "0.2.1",
    "class-validator": "0.12.2",
    "commander": "6.2.1",
    "cron": "1.8.2",
    "dotenv": "8.2.0",
    "inversify": "5.0.5",
    "knex": "^0.21.15",
    "mysql": "^2.18.1",
    "reflect-metadata": "0.1.13",
    "socket.io": "2.3.0",
    "sqlite3": "5.0.0",
    "tslib": "2.0.3",
    "typeorm": "0.2.29",
    "winston": "3.3.3"
  }
}
